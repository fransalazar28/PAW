@model IEnumerable<CatalogViewModel>


@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

@foreach (var c in Model)
{
    <div class="container">
        <div class="card">
            <div class="card-header">
                @c.Id
            </div>
            <div class="card-body">
                @c.Name
            </div>
        </div>
    </div>
}
